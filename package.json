{
  "name": "spleetpay-backend",
  "version": "1.0.0",
  "description": "Backend server for SpleetPay - Payment platform supporting Pay-for-Me and Group Split payments",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:rollback": "npx sequelize-cli db:migrate:undo",
    "seed": "npx sequelize-cli db:seed:all",
    "migration:create": "npx sequelize-cli migration:generate"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^2.6.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.2",
    "multer-s3": "^3.0.1",
    "mysql2": "^3.6.1",
    "nodemailer": "^6.10.1",
    "pg": "^8.11.3",
    "redis": "^4.6.12",
    "sequelize": "^6.33.0",
    "sequelize-cli": "^6.6.2",
    "socket.io": "^4.7.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.3",
    "winston": "^3.10.0",
    "aws-sdk": "^2.1508.0",
    "qrcode": "^1.5.3"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3",
    "@types/node": "^20.10.5"
  }
